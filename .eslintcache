 enrolledStudents.forEach((element) => {\n          fetch(\n            `https://nodeapi.beresearcherbd.com/api/assignment/${element._id}`\n          )\n            .then((resp) => resp.json())\n            .then((resp1) => {\n              setAssingments((prev) => {\n                // console.log(prev, resp1);\n                return Object.assign(prev, { [element._id]: resp1 });\n              });\n              // console.log(resp1);\n            });\n        });\n        setTimeout(() => {\n          setLoading(false);\n        }, 100);\n      })\n      .catch((err) => alert(err));\n  }, []);\n\n  return (\n    <BrowserRouter>\n      <div className=\"review-assignment\">\n        <div>\n          <h1>Review Assingments</h1>\n          {loading ? (\n            \"Loading...\"\n          ) : (\n            <table>\n              <thead>\n                <tr>\n                  <th>#</th>\n                  <th>Name</th>\n                  <th>Submitted Assignments</th>\n                </tr>\n              </thead>\n              <tbody>\n                {students.map((student, i) => (\n                  <tr key={i}>\n                    <td>{i}</td>\n                    <td>{student.name}</td>\n                    <td>\n                      <div\n                        style={{\n                          background: \"gray\",\n                          textAlign: \"center\",\n                          cursor: assignments[student._id] ? \"pointer\" : \"none\",\n                          padding: \"2px\",\n                          borderRadius: \"5px\",\n                        }}\n                        onClick={() => {\n                          assignments[student._id]\n                            ? setShow(!show)\n                            : setShow(show);\n                        }}\n                      >\n                        {show ? \"Hide\" : \"See\"}\n                      </div>\n\n                      {show === false ? (\n                        \"\"\n                      ) : (\n                        <ol>\n                          {assignments[student._id].map((element) => (\n                            <li key={element}>\n                              <Link\n                                to={`/admin/review-assignment/details/${student._id}/${element}`}\n                              >\n                                {element}\n                              </Link>\n                            </li>\n                          ))}\n                        </ol>\n                      )}\n                    </td>\n                  </tr>\n                ))}\n                {/* <tr>\n              <td>1</td>\n              <td>Mark</td>\n              <td>\n                <table>\n                  <tr>\n                    <a href=\"\">ja</a>\n                  </tr>\n                  <tr>no</tr>\n                  <tr>no</tr>\n                  <Link to=\"/admin/review-assignment/ken\">SO</Link>\n                </table>\n              </td>\n            </tr> */}\n              </tbody>\n            </table>\n          )}\n          {/* {students.map((student) => (\n            <pre>{JSON.stringify(student, null, 2)}</pre>\n          ))}\n          <pre>{JSON.stringify(assignments, null, 2)}</pre> */}\n        </div>\n        <Route\n          path=\"/admin/review-assignment/details/:studentId/:assignment\"\n          component={Details}\n        />\n      </div>\n    </BrowserRouter>\n  );\n};\nexport default ReviewAssignments;\n","/home/mateen/Desktop/BeResearcher/src/admin-panel/body-section/CreateAnnouncements.js",[],"/home/mateen/Desktop/BeResearcher/src/landing-page/common/Container/styles.js",[],"/home/mateen/Desktop/BeResearcher/src/landing-page/common/ScrollToTop/styles.js",[],"/home/mateen/Desktop/BeResearcher/src/landing-page/components/MiddleBlock/styles.js",[],"/home/mateen/Desktop/BeResearcher/src/landing-page/common/SvgIcon/index.js",[],"/home/mateen/Desktop/BeResearcher/src/